<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/feedbackccna/db" VERSION="20120725" COMMENT="XMLDB file for Moodle mod/feedbackccna"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="feedbackccna" COMMENT="Default comment for feedbackccna, please edit me" NEXT="feedback_module">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Course feedbackccna activity belongs to" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="name field for moodle instances" PREVIOUS="course" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="General introduction of the feedbackccna activity" PREVIOUS="name" NEXT="introformat"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Format of the intro field (MOODLE, HTML, MARKDOWN...)" PREVIOUS="intro" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="introformat" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="timecreated"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="feedback_module" COMMENT="Organizare pe saptamana" PREVIOUS="feedbackccna" NEXT="questions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="course_id"/>
        <FIELD NAME="course_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="section"/>
        <FIELD NAME="section" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" PREVIOUS="course_id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="questions" COMMENT="Default comment for the table, please edit me" PREVIOUS="feedback_module" NEXT="feedback_ccna">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="45" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="type"/>
        <FIELD NAME="type" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="which_way"/>
        <FIELD NAME="which_way" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" PREVIOUS="type"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="feedback_ccna" COMMENT="Default comment for the table, please edit me" PREVIOUS="questions" NEXT="answer">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="type"/>
        <FIELD NAME="type" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="45" NOTNULL="true" SEQUENCE="false" PREVIOUS="type" NEXT="instructor_id"/>
        <FIELD NAME="instructor_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="module_id"/>
        <FIELD NAME="module_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="instructor_id" NEXT="allow"/>
        <FIELD NAME="allow" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" PREVIOUS="module_id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="fk_module_id"/>
        <KEY NAME="fk_module_id" TYPE="foreign" FIELDS="module_id" REFTABLE="feedback_module" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="answer" COMMENT="Default comment for the table, please edit me" PREVIOUS="feedback_ccna">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="student_id"/>
        <FIELD NAME="student_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="feedback_id"/>
        <FIELD NAME="feedback_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="student_id" NEXT="question_id"/>
        <FIELD NAME="question_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="feedback_id" NEXT="answer"/>
        <FIELD NAME="answer" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" PREVIOUS="question_id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="fk_feedback_id"/>
        <KEY NAME="fk_feedback_id" TYPE="foreign" FIELDS="feedback_id" REFTABLE="feedback_ccna" REFFIELDS="id" PREVIOUS="primary" NEXT="fk_questions_id"/>
        <KEY NAME="fk_questions_id" TYPE="foreign" FIELDS="question_id" REFTABLE="questions" REFFIELDS="id" PREVIOUS="fk_feedback_id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>